#include <stdio.h>
int main()
{
    int r,c;
    printf("Enter matrix row size: ");
    scanf("%d",&r);
    printf("Enter matrix column size: ");
    scanf("%d",&c);
    int mat[r][c];
    printf("Enter matrix elements:\n");
    for (int i=0; i<r; i++)
    {
        for (int j=0; j<c; j++)
        {
            scanf("%d",&mat[i][j]);
        }
    }
    printf("Given matrix:\n");
    for (int i=0; i<r; i++)
    {
        for (int j=0; j<c; j++)
        {
            printf("%d ",mat[i][j]);
        }
        printf("\n");
    }
    printf("Interchange matrix:\n");
    int temp;
    for (int i=0; i<r; i++)
    {
        temp=mat[i][0];
        mat[i][0]=mat[i][c-1];
        mat[i][c-1]=temp;
    }
    printf("Interchanged matrix:\n");
    for (int i=0; i<r; i++)
    {
        for (int j=0; j<c; j++)
        {
            printf("%d ",mat[i][j]);
        }
        printf("\n");
    }
    return 0;
}
