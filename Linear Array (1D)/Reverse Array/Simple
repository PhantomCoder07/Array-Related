#include <stdio.h>
int main()
{
    int i,n;
    printf ("Enter size of array: ");
    scanf ("%d",&n);
    int arr[n];
    printf ("Enter %d numbers: ",n);
    for (i=0; i<n; i++)
    {
        scanf ("%d",&arr[i]);
    }
    printf ("The given numbers in reverse order: ");
    for (i=n-1; i>=0; i--)
    {
        printf ("%d ",arr[i]);
    }
    return 0;
}
