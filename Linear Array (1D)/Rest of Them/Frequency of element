#include <stdio.h>
int main()
{
    int n,i,j,c;
    printf ("Enter array size: ");
    scanf ("%d",&n);
    int arr[n], frq[n];
    printf ("Enter %d numbers: ",n);
    for (i=0; i<n; i++)
    {
        scanf ("%d",&arr[i]);
        frq[i]=-1;
    }
    printf("Given numbers: ");
    for (i=0; i<n; i++)
    {
        printf ("%d ",arr[i]);
    }
    for (i=0; i<n; i++)
    {
        c=1;
        for (j=i+1; j<n; j++)
        {
            if (arr[i]==arr[j])
            {
                c++;
                frq[j]=0;
            }
        }
        if (frq[i]!=0)
        {
            frq[i]=c;
        }
    }
    printf ("\nThe frequency of all elements of the array : \n");
    for (i=0; i<n; i++)
    {
        if (frq[i]!=0)
        {
            printf ("%d occurs %d times\n", arr[i], frq[i]);
        }
	}
	return 0;
}
