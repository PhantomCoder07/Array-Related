#include <stdio.h>
int main()
{
    int n,i, sum1,sum2=0;
    printf ("Enter n term: ");
    scanf ("%d",&n);
    sum1= (n*(n+1))/2;
    int arr[n-1];
    printf ("Enter first %d numbers excluding a random number: ",n);
    for (i=0; i<n-1; i++)
    {
        scanf ("%d",&arr[i]);
        sum2+=arr[i];
    }
    printf ("The numbers are: ");
    for (i=0; i<n-1; i++)
    {
        printf ("%d ",arr[i]);
    }
    printf ("\nThe missing number: %d\n",sum1-sum2);
    return 0;
}
