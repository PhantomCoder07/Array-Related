#include <stdio.h>
int main()
{
    int n,s;
    printf ("Enter array limit: ");
    scanf ("%d",&n);
    int arr[n];
    printf ("Enter array element: ");
    for (int i=0; i<n; i++)
    {
        scanf ("%d",&arr[i]);
    }
    printf ("Enter search element: ");
    scanf ("%d",&s);
    printf ("The array without value %d: ",s);
    for (int i=0; i<n; i++)
    {
        if (arr[i]!=s)
            printf ("%d ",arr[i]);
    }
    return 0;
}
